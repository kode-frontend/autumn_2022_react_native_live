{
  "name": "frontend-intership-2022",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "run-expo": "expo start",
    "run-android": "expo start --android",
    "run-ios": "expo start --ios",
    "run-web": "expo start --web",
    "start": "npm-run-all prestart run-expo",
    "android": "npm-run-all prestart run-android",
    "ios": "npm-run-all prestart run-ios",
    "web": "npm-run-all prestart run-web",
    "update-icons": "ts-node-transpile-only --project node.tsconfig.json scripts/transform-svg/transform-svg.ts",
    "prestart": "sb-rn-get-stories",
    "storybook-watcher": "sb-rn-watcher"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-community/datetimepicker": "6.2.0",
    "@react-native-community/slider": "4.2.3",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.0",
    "@storybook/addon-controls": "6.3",
    "@storybook/addon-ondevice-backgrounds": "^6.0.1-beta.9",
    "@storybook/addon-ondevice-controls": "^6.0.1-beta.9",
    "@storybook/addon-ondevice-notes": "^6.0.1-beta.9",
    "expo": "~46.0.13",
    "expo-font": "^10.2.1",
    "expo-splash-screen": "^0.16.2",
    "expo-status-bar": "~1.4.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.6",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "~3.15.0",
    "react-native-svg": "12.3.0",
    "react-native-web": "~0.18.7",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@storybook/addon-actions": "6.3",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addon-ondevice-actions": "^6.0.1-beta.9",
    "@storybook/react-native": "^6.0.1-beta.9",
    "@storybook/react-native-server": "^5.3.23",
    "@svgr/core": "^6.4.0",
    "@types/prettier": "^2.7.1",
    "@types/react": "~18.0.14",
    "@types/react-native": "~0.69.1",
    "@types/styled-components": "^5.1.26",
    "@types/styled-components-react-native": "^5.1.3",
    "babel-loader": "^8.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "react-native-storybook-loader": "^2.0.5",
    "ts-node": "^10.9.1",
    "typescript": "~4.3.5"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.story.tsx",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "private": true
}
